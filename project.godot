config_version=5

[application]

config/name="MegaRedone: Rebirth (Engine Wip)"
config/tags=PackedStringArray("engine", "megaman", "rebirth")
run/main_scene="res://levels/beginning_screen.tscn"
config/features=PackedStringArray("4.3.2", "GL Compatibility")
run/max_fps=60
boot_splash/image="res://assets/sprites/miscelleaneous/new_engine_load_screen.png"
config/icon="res://assets/sprites/miscelleaneous/Mega_Man_Redone_Rebirth_logo_icon.png"

[aseprite]

import/import_plugin/default_automatic_importer="SpriteFrames"

[autoload]

StageFunctions="*res://GlobalScripts/StageFunctions.gd"
GlobalScript="*res://GlobalScripts/GlobalScript.gd"
MegamanAndItems="*res://GlobalScripts/megaman_and_items.tscn"
GlobalScreenTransitionTimer="*res://GlobalScripts/global_screen_transition_timer.tscn"
InterStageScene="*res://levels/inter_stage_scene.tscn"

[display]

window/size/viewport_width=256
window/size/viewport_height=224
window/energy_saving/keep_screen_on=false
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="MegaRedone- Rebirth (Engine Wip)"

[editor_plugins]

enabled=PackedStringArray("res://addons/AsepriteWizard/plugin.cfg", "res://addons/new_camera_system/plugin.cfg")

